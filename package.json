{
  "name": "ellipsa",
  "private": true,
  "version": "0.1.0",
  "description": "ellipsa v1 monorepo",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev:memory": "pnpm --filter @ellipsa/memory dev",
    "dev:processor": "pnpm --filter @ellipsa/processor dev",
    "dev:prompt": "pnpm --filter @ellipsa/prompt-service dev",
    "dev:action": "pnpm --filter @ellipsa/action dev",
    "dev:all": "concurrently -n mem,proc,prompt,act -c blue,magenta,green,yellow \"pnpm dev:memory\" \"pnpm dev:processor\" \"pnpm dev:prompt\" \"pnpm dev:action\"",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/electron": "^1.6.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.0.0",
    "google-auth-library": "^10.4.2",
    "googleapis": "^164.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.5.4",
    "ws": "^8.17.0",
    "zod": "^3.23.8"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "dependencies": {
    "axios": "^1.12.2"
  }
}
