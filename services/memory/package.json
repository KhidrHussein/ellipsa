{
  "name": "@ellipsa/memory",
  "version": "0.1.0",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "cross-env NODE_ENV=development tsx watch src/server.ts",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:rollback": "knex migrate:rollback",
    "seed:run": "knex seed:run",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests --runInBand",
    "test:watch": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --runInBand",
    "test:websocket": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js src/test/websocket.test.ts --runInBand --detectOpenHandles --forceExit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@ellipsa/shared": "workspace:*",
    "@neondatabase/serverless": "^1.0.0",
    "@types/pg": "^8.11.2",
    "better-sqlite3": "^9.4.3",
    "chromadb": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.19.2",
    "neo4j-driver": "^5.15.0",
    "openai": "^4.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.14.12",
    "@types/socket.io": "^3.0.2",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "ws": "^8.17.0"
  }
}
